        <outputendpoint name="assignee-list" type="text" label="Issue assignee list" friendcode="jira-assignee-list list"/>
        <outputendpoint name="type-list" type="text" label="Issue type list" friendcode="jira-type-list list"/>
    </wiring>

    <scripts>
        <script src="js/main.js"/>
    </scripts>

</operator>
